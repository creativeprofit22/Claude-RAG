{
  "name": "claude-rag",
  "version": "1.0.0",
  "description": "RAG system with Claude Code CLI integration and React chat UI",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./react": {
      "types": "./dist/react/index.d.ts",
      "import": "./dist/react/index.js"
    },
    "./react/styles.css": "./dist/react/styles.css"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsc && npm run copy-assets",
    "build:demo": "esbuild demo/browser-entry.tsx --bundle --format=iife --global-name=RAGBundle --target=es2020 --outfile=demo/ragchat-bundle.js --alias:react=./demo/react-shim.js --alias:react-dom=./demo/react-shim.js --alias:lucide-react=./demo/lucide-shim.js --alias:react/jsx-runtime=./demo/jsx-runtime-shim.js --alias:react/jsx-dev-runtime=./demo/jsx-runtime-shim.js",
    "copy-assets": "cp src/react/styles.css dist/react/styles.css && rm -rf dist/react/tokens dist/react/fonts && cp -r src/react/tokens dist/react/tokens && cp -r src/react/fonts dist/react/fonts",
    "dev": "bun run src/server.ts",
    "start": "bun run src/server.ts",
    "dev:tsc": "tsc --watch",
    "test": "bun test",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "rag",
    "claude",
    "ai",
    "vector-database",
    "lancedb",
    "chat",
    "react",
    "embeddings"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@google/genai": "^1.34.0",
    "@gsap/react": "^2.1.2",
    "@lancedb/lancedb": "^0.23.0",
    "echarts": "^6.0.0",
    "echarts-for-react": "^3.0.5",
    "framer-motion": "^12.23.26",
    "gsap": "^3.14.2",
    "lucide-react": "^0.468.0",
    "mammoth": "^1.9.0",
    "pdf-parse": "^1.1.1",
    "xlsx": "^0.18.5"
  },
  "peerDependencies": {
    "react": "^18.0.0 || ^19.0.0",
    "react-dom": "^18.0.0 || ^19.0.0"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": true
    },
    "react-dom": {
      "optional": true
    }
  },
  "devDependencies": {
    "@types/echarts": "^4.9.22",
    "@types/node": "^22.10.2",
    "@types/pdf-parse": "^1.1.4",
    "@types/react": "^18.3.14",
    "@types/react-dom": "^18.3.1",
    "esbuild": "^0.27.2",
    "typescript": "^5.7.2"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/creativeprofit22/Claude-RAG.git"
  },
  "bugs": {
    "url": "https://github.com/creativeprofit22/Claude-RAG/issues"
  },
  "homepage": "https://github.com/creativeprofit22/Claude-RAG#readme"
}
