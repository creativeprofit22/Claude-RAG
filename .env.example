# Required: Google AI API key for embeddings
# Get one at https://aistudio.google.com/apikey
GOOGLE_AI_API_KEY=your-google-ai-api-key

# Optional: Gemini embedding model (default: gemini-embedding-001)
GEMINI_EMBEDDING_MODEL=gemini-embedding-001

# Optional: Embedding output dimensionality (default: 1024)
# Supported: 256, 512, 768, 1024, 1536, 3072
GEMINI_EMBEDDING_DIM=1024

# Optional: Vector database path
RAG_DB_PATH=./data/vectors

# Optional: Server port (default: 3000)
PORT=3000
